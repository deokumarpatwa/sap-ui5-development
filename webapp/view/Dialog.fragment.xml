<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Dialog
        id="dialog"
        title="Info"
        type="Message"
    >
        <content>
            <Text text="Customer {customer>/CustomerName} data is later saved via an oData service." />
        </content>
        <beginButton>
            <Button
                text="Ok"
                press=".onCloseDialog"
            />
        </beginButton>
    </Dialog>
</core:FragmentDefinition>
